#include "main.h"
/**
 * print_last_digit - Prints last digit of a number
 *@n: is the int that will use for the argument of the function
 * Return: last digit
 */
int print_last_digit(int n)
{
	int x;	

	last = (n % 10);

	if (last < 0)
		last = last * -1;

	_putchar(last + '0');
	return (last);
}
